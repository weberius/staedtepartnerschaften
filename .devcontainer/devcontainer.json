{
  "name": "isolated-ubuntu",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "workspaceFolder": "/workspaces/staedtepartnerschaften",
  "runArgs": [
    "--cap-drop=ALL",
    "--security-opt=no-new-privileges:true"
  ],
  "features": {},
  "hostRequirements": {
    "gpu": false
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "bierner.markdown-mermaid",
        "eamodio.gitlens",
        "mhutchie.git-graph",
        "fwcd.kotlin",
        "continue.continue",
        "ms-azuretools.vscode-docker"
      ]
    }
  },
  "mounts": [],
  "remoteEnv": {
    "DISPLAY": null
  },
  "containerEnv": {
    "WAYLAND_DISPLAY": null
  }
}